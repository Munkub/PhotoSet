<Window x:Class="PhotoSet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoSet"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid>
            <Grid.Background>
                <SolidColorBrush Color="#DCDCDC "/>
            </Grid.Background>
            <TextBlock Text="Авторизация" Margin="150,23,150,369" TextAlignment="Center" FontSize="22" Background="#C0C0C0"/>
            <TextBlock Margin="178,94,89,322"><Run Language="ru-ru" Text="Пароль"/></TextBlock>
            <TextBox x:Name="LoginTbx" Margin="178,117,178,296" FontSize="14" Background="#C0C0C0"/>
            <TextBlock Margin="178,156,175,262"><Run Language="ru-ru" Text="Логин"/></TextBlock>
            <TextBox x:Name="PasswordTbx" Margin="178,174,178,239" FontSize="14" Background="#C0C0C0"/>
            <Button x:Name="Voyti" Content="Войти" Margin="269,263,297,125" Background="#A79AFF" Click="Voyti_Click"/>
            <Button x:Name="Registration" Content="Регистрация" Margin="226,335,238,53" Background="#A79AFF" Click="Registration_Click"/>
        </Grid>
    </Grid>
</Window>
