<Window x:Class="PhotoSet.ThingsClients"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoSet"
        mc:Ignorable="d"
        Title="ThingsClients" Height="450" Width="800">
    <Grid>
        <Grid Background="#DCDCDC">
        </Grid>
        <DataGrid x:Name="ThingsDG" Margin="47,67,37,130"/>
        <TextBlock Text="Товары и услуги" Margin="120,18,118,382" FontSize="24" TextAlignment="Center"/>
        <Button x:Name="BackBtn" Content="Назад" Margin="274,339,266,57" FontSize="20" Click="BackBtn_Click" >
            <Button.Style>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <Border CornerRadius="5" Background="#A79AFF">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </Border>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Button.Style>
        </Button>
    </Grid>
</Window>
